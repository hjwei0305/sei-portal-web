{
  "name": "sei-portal-web",
  "version": "1.0.0",
  "private": true,
  "description": "the portal of applications",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "umi build",
    "format-imports": "cross-env import-sort --write '**/*.{js,jsx,ts,tsx}'",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "check-prettier lint",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "prettier": "prettier -c --write \"**/*\"",
    "start": "umi dev",
    "start:no-mock": "cross-env MOCK=none umi dev",
    "start:no-ui": "cross-env UMI_UI=none umi dev"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.less": "stylelint --fix --syntax less",
    "src/**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.{js,jsx,ts,tsx}": "eslint --cache --fix --format=pretty"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@umijs/plugin-qiankun": "^1.5.1",
    "antd": "^3.23.6",
    "classnames": "^2.2.6",
    "dva": "^2.6.0-beta.16",
    "events": "^3.1.0",
    "fetch-polyfill": "^0.8.2",
    "focus-outside": "^0.5.3",
    "html2canvas": "^1.0.0-rc.5",
    "less-vars-to-js": "^1.3.0",
    "lodash": "^4.17.11",
    "md5": "^2.2.1",
    "moment": "^2.24.0",
    "nprogress": "^0.2.0",
    "prop-types": "^15.7.2",
    "query-string": "^6.12.1",
    "rc-resize-observer": "^0.2.1",
    "rc-trigger": "^2.6.2",
    "react": "^16.8.6",
    "react-color": "^2.18.1",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-transition-group": "^4.4.1",
    "screenfull": "^5.0.0",
    "suid": "^1.3.25",
    "umi": "^2.8.7",
    "umi-plugin-react": "^1.9.5"
  },
  "devDependencies": {
    "@umijs/fabric": "2.0.0",
    "chalk": "^3.0.0",
    "check-prettier": "^1.0.3",
    "cross-env": "^6.0.0",
    "cross-port-killer": "^1.1.1",
    "husky": "^3.0.0",
    "import-sort-cli": "^6.0.0",
    "import-sort-parser-babylon": "^6.0.0",
    "import-sort-parser-typescript": "^6.0.0",
    "import-sort-style-module": "^6.0.0",
    "lint-staged": "^9.0.0",
    "prettier": "^1.19.1",
    "stylelint": "^12.0.0"
  },
  "optionalDependencies": {
    "less-vars-to-js": "^1.3.0",
    "puppeteer": "^2.0.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "checkFiles": [
    "src/**/*.js*",
    "src/**/*.ts*",
    "src/**/*.less",
    "config/**/*.js*",
    "scripts/**/*.js"
  ]
}
